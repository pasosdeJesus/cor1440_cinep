<% col = Sip::Orgsocial.joins(:grupoper).where(
  'sip_orgsocial.id IN (SELECT DISTINCT orgsocial_id ' + 
  '  FROM cor1440_gen_actividad_orgsocial)') %>
<%= f.input :busorgsocial,
  collection: col.order('sip_grupoper.nombre'),
  include_blank: true,
  label_method: :presenta_nombre,
  value_method: :id,
  label: false,
  selected: Sip::ModeloHelper.poromision(params, :busorgsocial),
  input_html: {
    class: 'chosen-select',
    'data-enviarautomatico' => ''
  }
%>
