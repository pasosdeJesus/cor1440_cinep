<h1 style='text-align:center'><%= @titulo %></h1>
<h2>Usuarios encuestados: <%= @usuarios.count %></h2>

<h3>Consolidados de las respuestas</h2>
<table class="table table-striped">
  <tr>
    <th>Pregunta</th>
    <th>Consolidado</th>
  </tr>
  <% @consolidado.each do |c| %>
    <tr>
      <td><%= c[:pregunta] %></td>
      <td><%= c[:consolidado] %></td>
    </tr>
  <% end %>
</table>
