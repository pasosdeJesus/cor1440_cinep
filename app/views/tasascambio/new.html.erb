<%- model_class = clase.constantize -%>
<div class="page-header">
	<% n = !genclase.nil? && genclase == 'F' ?  'Nueva' : 'Nuevo' %>
  <h1><%=t '.title', :default => [ n +' %{model}', :'helpers.titles.new', 
    'New %{model}'], 
    :model => model_class.human_attribute_name(model_class.model_name.to_s.demodulize) 
    %></h1>
</div>
<div class="row">
  <div class="col-sm-8">
    <%= render :partial => 'form' %>
  </div>
  <div class="col-sm-4 alert"> 
    <table class='table table-striped table-bordered table-inverse'>
      <thead><tr><th>Moneda</th><th>Tasa hoy</th></tr></thead>
      <tbody>
        <% @div.each { |k, v| %>
          <tr><td><%= k %></td><td><%= v %></td></tr>
        <% } %>
      </tbody>
    </table>
    Valores de http://www.apilayer.net/
  </div>
</div>
