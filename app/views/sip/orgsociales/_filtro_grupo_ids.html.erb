<% col = Sip::Grupo.where(
  'id IN (SELECT DISTINCT grupo_id FROM grupo_orgsocial)') %>
<% puts "listado org. sociales, grupos: #{col.count}" %>
<%= f.input :busgrupo_ids,
  collection: col.order(:nombre),
  include_blank: true,
  label_method: :nombre,
  value_method: :id,
  label: false,
  selected: Sip::ModeloHelper.poromision(params, :busgrupo_ids),
  input_html: {
    'data-enviarautomatico' => ''
  }
%>
